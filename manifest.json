{
    "manifest_version": 3,
    "name": "Class Tracker",
    "version": "1.0",
    "description": "Track and access your classes, assignments, and discussions quickly",
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "permissions": ["storage", "sidePanel"],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }